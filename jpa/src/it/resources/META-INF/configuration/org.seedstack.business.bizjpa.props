org.seedstack.seed.persistence.jpa.units = seed-biz-support

[org.seedstack.seed.persistence.jpa.unit.seed-biz-support.property]
javax.persistence.jdbc.driver=org.hsqldb.jdbcDriver
javax.persistence.jdbc.url=jdbc:hsqldb:mem:testdb
javax.persistence.jdbc.user=sa
javax.persistence.jdbc.password=
hibernate.dialect=org.hibernate.dialect.HSQLDialect
hibernate.hbm2ddl.auto=create
sql.enforce_strict_size=true

[org.seedstack.business.jpa.samples.domain.identity.MyAggregate]
identity.handler-qualifier = inmemory-sequence
identity.sequence-name = SEQTEST

[org.seedstack.business.jpa.samples.domain.identity.MyEntity]
identity.handler-qualifier = simple-UUID

[org.seedstack.business.event]
domain.watch=true